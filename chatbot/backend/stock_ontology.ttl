@prefix : <http://kg-demo.org/stock#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:Company a owl:Class ;
    rdfs:label "Company" ;
    rdfs:comment "A publicly traded company" .

:PriceDay a owl:Class ;
    rdfs:label "Price Day" ;
    rdfs:comment "A trading day with price data" .

:Year a owl:Class ;
    rdfs:label "Year" .

:Quarter a owl:Class ;
    rdfs:label "Quarter" .

:Month a owl:Class ;
    rdfs:label "Month" .

:symbol a owl:DatatypeProperty ;
    rdfs:domain :Company ;
    rdfs:range xsd:string .

:date a owl:DatatypeProperty ;
    rdfs:domain :PriceDay ;
    rdfs:range xsd:date .

:open a owl:DatatypeProperty ;
    rdfs:domain :PriceDay ;
    rdfs:range xsd:float .

:high a owl:DatatypeProperty ;
    rdfs:domain :PriceDay ;
    rdfs:range xsd:float .

:low a owl:DatatypeProperty ;
    rdfs:domain :PriceDay ;
    rdfs:range xsd:float .

:close a owl:DatatypeProperty ;
    rdfs:domain :PriceDay ;
    rdfs:range xsd:float .

:adj_close a owl:DatatypeProperty ;
    rdfs:domain :PriceDay ;
    rdfs:range xsd:float .

:volume a owl:DatatypeProperty ;
    rdfs:domain :PriceDay ;
    rdfs:range xsd:integer .

:year a owl:DatatypeProperty ;
    rdfs:domain :Year ;
    rdfs:range xsd:integer .

:hasPrice a owl:ObjectProperty ;
    rdfs:domain :Company ;
    rdfs:range :PriceDay ;
    rdfs:label "Has Price" .

:correlatedWith a owl:ObjectProperty ;
    rdfs:domain :Company ;
    rdfs:range :Company ;
    rdfs:label "Correlated With" .

:performedIn a owl:ObjectProperty ;
    rdfs:domain :Company ;
    rdfs:range :Year ;
    rdfs:label "Performed In" .

:inYear a owl:ObjectProperty ;
    rdfs:domain :PriceDay ;
    rdfs:range :Year .

:inQuarter a owl:ObjectProperty ;
    rdfs:domain :PriceDay ;
    rdfs:range :Quarter .

:inMonth a owl:ObjectProperty ;
    rdfs:domain :PriceDay ;
    rdfs:range :Month .

:Sector a owl:Class ;
    rdfs:label "Sector" ;
    rdfs:comment "A business sector or industry category" .

:Country a owl:Class ;
    rdfs:label "Country" ;
    rdfs:comment "A country location" .

:State a owl:Class ;
    rdfs:label "State" ;
    rdfs:comment "A state or province within a country" .

:City a owl:Class ;
    rdfs:label "City" ;
    rdfs:comment "A city location" .

:name a owl:DatatypeProperty ;
    rdfs:domain :Sector, :Country, :State, :City ;
    rdfs:range xsd:string .

:inSector a owl:ObjectProperty ;
    rdfs:domain :Company ;
    rdfs:range :Sector ;
    rdfs:label "In Sector" .

:locatedIn a owl:ObjectProperty ;
    rdfs:domain :Company ;
    rdfs:range :Country ;
    rdfs:label "Located In" .

:inState a owl:ObjectProperty ;
    rdfs:domain :Company, :City ;
    rdfs:range :State ;
    rdfs:label "In State" .

:inCity a owl:ObjectProperty ;
    rdfs:domain :Company ;
    rdfs:range :City ;
    rdfs:label "In City" .

:inCountry a owl:ObjectProperty ;
    rdfs:domain :State ;
    rdfs:range :Country ;
    rdfs:label "In Country" .

:gdsSimilar a owl:ObjectProperty ;
    rdfs:domain :Company ;
    rdfs:range :Company ;
    rdfs:label "GDS Similar" ;
    rdfs:comment "Graph Data Science Node Similarity relationship" .

:gdsSimilarScore a owl:DatatypeProperty ;
    rdfs:domain :gdsSimilar ;
    rdfs:range xsd:float ;
    rdfs:label "GDS Similarity Score" .

:pagerank a owl:DatatypeProperty ;
    rdfs:domain :Company ;
    rdfs:range xsd:float ;
    rdfs:label "PageRank" ;
    rdfs:comment "Graph Data Science PageRank centrality score" .

:community a owl:DatatypeProperty ;
    rdfs:domain :Company ;
    rdfs:range xsd:integer ;
    rdfs:label "Community" ;
    rdfs:comment "Graph Data Science Louvain community detection result" .

:embedding a owl:DatatypeProperty ;
    rdfs:domain :Company ;
    rdfs:range rdf:List ;
    rdfs:label "Embedding" ;
    rdfs:comment "Graph Data Science FastRP embedding vector" .
